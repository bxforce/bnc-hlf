{
  "name": "bnc",
  "version": "0.0.1",
  "description": "The CLI deployment tool for Hyperledger enterprise blockchains",
  "main": "index.js",
  "private": true,
  "cacheDirectories": [
    "node_modules"
  ],
  "files": [
    "bin/*",
    "dist/*",
    "docs/*",
    "templates/*",
    "network-objects/*",
    "tools/*",
    "tests/manual/templates/docker-compose.yml",
    "package.json"
  ],
  "bin": {
    "bnc": "./dist/command.js"
  },
  "engines": {
    "node": ">=12.15.1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "watch": "npm run clean && tsc -w",
    "prepare": "npm run build",
    "lint": "tslint --fix -c './tslint.json' -p -p './tsconfig.json'",
    "test": "jest --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bxforce/bnc-hlf.git"
  },
  "keywords": [
    "bnc",
    "development",
    "hyperledger",
    "fabric",
    "environment",
    "blockchain",
    "hyperledger fabric"
  ],
  "authors": [
    {
      "name": "Wassim Znaidi",
      "email": "wassim.znaidi@gmail.com"
    },
    {
      "name": "Sahar Fehri",
      "email": "sahar.fehri@irt-systemx.fr"
    },
    {
      "name": "Ahmed Souissi",
      "email": "ahmed.souissi@irt-systemx.fr"
    },
    {
      "name": "Chiraz Chaabane",
      "email": "chiraz.chaabane@irt-systemx.fr"
    }
  ],
  "dependencies": {
    "@types/dockerode": "^2.5.24",
    "async": "^2.1.4",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "commander": "^5.0.0",
    "docker-compose": "^0.23.3",
    "dockerode": "^3.1.0",
    "ejs": "^3.0.1",
    "fabric-ca-client": "^2.1.0",
    "fabric-client": "^1.4.9",
    "fabric-network": "^2.1.0",
    "fs-extra": "^9.0.0",
    "grpc": "^1.24.2",
    "inquirer": "^7.0.4",
    "js-yaml": "^3.14.0",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^6.0.0",
    "node-docker-api": "^1.1.22",
    "path": "^0.12.7",
    "shelljs": "^0.8.3",
    "sudo-prompt": "^9.1.1",
    "tslib": "^1.10.0",
    "yaml": "^1.7.2",
    "yaml-validator": "^2.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^26.0.0",
    "@types/js-yaml": "^3.12.2",
    "@types/node": "^12.12.27",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "jest": "^26.0.1",
    "prettier": "1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.6.2",
    "tsc": "^1.20150623.0",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": "(/test/.*|\\.(spec))\\.(ts|js)$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  }
}
